<?xml version = "1.0" encoding = "UTF-8" ?>
<waitingService:WaitableApplication xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Planner.Maui"
             xmlns:waitingService="clr-namespace:Melville.MVVM.Maui.WaitingService;assembly=Melville.MVVM.Maui"
             x:Class="Planner.Maui.App">
    <waitingService:WaitableApplication.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </waitingService:WaitableApplication.Resources>
</waitingService:WaitableApplication>
